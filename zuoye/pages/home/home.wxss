.wechat{
  width: 680rpx;
  height: 680rpx;
}
.perform,.evaluate{
  height: 1rpx;
  border: 1px solid #ddd;
}
.perform::before{
  content: '演艺经历';
  position: relative;
  top:-25rpx;
  height: 50rpx;
  line-height: 50rpx;
  background-color:#fff;
  padding:0 40rpx;
}
.evaluate::before{
  content: '自我评价';
  position: relative;
  top:-25rpx;
  height: 50rpx;
  line-height: 50rpx;
  background-color:#fff;
  padding:0 40rpx;
}